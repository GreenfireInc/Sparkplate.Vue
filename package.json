{
  "name": "Sparkplate.Vue",
  "version": "0.0.1",
  "main": "background/main/index.js",
  "description": "A Really simple boilerplate based on Electron + Vue + Vite boilerplate and stripped down version of Greenery.",
  "author": "The Sparkplate team, a subdivision of Greenfire <Hello@greenfire.io>",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "boilerplate",
    "vue3",
    "vue"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "gbuild": "vue-cli-service electron:build",
    "gdev": "vue-cli-service electron:serve",
    "glint": "vue-cli-service lint",
    "preview": "vite preview"

  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.3",
    "vite-plugin-electron": "^0.11.1",
    "vite-plugin-electron-renderer": "^0.11.3",
    "vue": "^3.2.45",
    "vue-tsc": "^1.0.16"
  }
}
