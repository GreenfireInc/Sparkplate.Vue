{
  "name": "sparkplate",
  "description": "Based on Greenery, a Greenfire boilerplate for future products",
  "version": "1.0.0",
  "private": true,
  "author": "The Greenery team, a subdivision of Greenfire <hello@greenfire.io> (http://greenfire.io)",
  "main": "dist/background/index.cjs",
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:8080/"
    }
  },
  "scripts": {
    "dev": "vite",
    "dev:force": "vite --force",
    "build": "cross-env NODE_ENV=production npm run build:vite && electron-builder build --config .electron-builder.config.js",
    "build:dir": "cross-env NODE_ENV=production npm run build:vite && electron-builder build --config .electron-builder.config.js --dir",
    "build:vite": "vite build",
    "lint": "eslint --ext .js,.vue --fix src background",
    "format": "prettier .  --write",
    "patch": "npm run patch:@ensdomains/address-encoder && npm run patch:pbkdf2 && npm run patch:@taquito/http-utils",
    "patch:@ensdomains/address-encoder": "cp -f ./patches/@ensdomains/address-encoder/package.json ./node_modules/@ensdomains/address-encoder/package.json",
    "patch:@taquito/http-utils": "cp -f ./patches/@taquito/http-utils/package.json ./node_modules/@taquito/http-utils/package.json",
    "patch:pbkdf2": "cp -f ./patches/pbkdf2/lib/default-encoding.js ./node_modules/pbkdf2/lib/default-encoding.js",
    "postinstall": "electron-builder install-app-deps && npm run patch && patch-package",
    "postuninstall": "electron-builder install-app-deps",
    "electron:generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten"
  },
  "dependencies": {
    "@airgap/beacon-sdk": "^3.0.0",
    "@ensdomains/ensjs": "^2.1.0",
    "@json2csv/node": "^7.0.1",
    "@pdf-lib/fontkit": "^1.1.1",
    "@taquito/rpc": "~17.0.0",
    "@taquito/signer": "~17.0.0",
    "@taquito/taquito": "~17.0.0",
    "@taquito/tzip16": "~17.0.0",
    "@taquito/utils": "~17.0.0",
    "@tezos-domains/core": "~1.26.0",
    "@tezos-domains/taquito-client": "~1.26.0",
    "@unstoppabledomains/resolution": "^7.1.4",
    "@walletconnect/client": "^1.8.0",
    "@walletconnect/core": "^2.5.2",
    "@walletconnect/utils": "^2.6.2",
    "@walletconnect/web3wallet": "^1.5.0",
    "@wert-io/widget-initializer": "^2.0.3",
    "@zxing/browser": "0.0.9",
    "archiver": "^5.1.0",
    "archiver-zip-encryptable": "^1.0.7",
    "axios": "^0.21.1",
