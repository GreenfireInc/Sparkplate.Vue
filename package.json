    "country-state-city": "^1.0.5",
    "crypto-browserify": "^3.12.0",
    "crypto-js": "^4.1.1",
    "csv-writer": "^1.6.0",
    "csvtojson": "^2.0.10",
    "currency-codes": "^2.1.0",
    "currency-symbol-map": "^5.0.1",
    "ed25519-hd-key": "^1.2.0",
    "ethers": "^5.7.2",
